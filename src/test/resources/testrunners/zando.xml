<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Zando E2E test suite" parallel="classes" thread-count="2">
    <test name ="ProfileTest">
        <parameter name="browser" value="${browser}">
        <parameter name="baseUrl" value="${baseUrl}">
            <classes>
                <class name="tests.profileTests.ProfileTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
    <test name ="ShoppingCartTests">
        <parameter name="browser" value="${browser}">
        <parameter name="baseUrl" value="${baseUrl}">
            <classes>
                <class name="tests.shoppingCartTests.ShoppingCartTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
    <test name ="SearchTests">
        <parameter name="browser" value="${browser}">
        <parameter name="baseUrl" value="${baseUrl}">
            <classes>
                    <class name="tests.searchTests.SearchTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
</suite>