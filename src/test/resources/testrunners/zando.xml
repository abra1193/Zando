<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Zando E2E test suite" parallel="classes" thread-count="2">
    <test name ="ProfileTest">
        <parameter name="browser" value="chrome">
        <parameter name="baseUrl" value="https://en.zalando.de/">
            <classes>
                <class name="tests.profileTests.ProfileTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
    <test name ="OrderTests">
        <parameter name="browser" value="chrome">
        <parameter name="baseUrl" value="https://en.zalando.de/">
            <classes>
                <class name="tests.orderTests.OrderTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
    <test name ="SearchTests">
        <parameter name="browser" value="chrome">
        <parameter name="baseUrl" value="https://en.zalando.de/">
            <classes>
                    <class name="tests.searchTests.SearchTests"/>
            </classes>
        </parameter>
        </parameter>
    </test>
</suite>